{
    "name": "hugoseigle/symfony-import-export-bundle",
    "description": "Symfony bundle for importing and exporting entities",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Hugo SEIGLE",
            "email": "hugo.seigle03@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "doctrine/orm": "^3.2",
        "phpoffice/phpspreadsheet": "^1.23",
        "symfony/form": "^7.1",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "symfony/translation": "^7.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.44",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpro/grumphp": "^2.9",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^10.5",
        "symfony/test-pack": "^1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SymfonyImportExportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyImportExportBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpro/grumphp": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.* || 7.*"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix",
        "test": "phpunit --configuration phpunit.xml.dist"
    }
}
