{
    "name": "hugoseigle/symfony-import-export-bundle",
    "version": "1.0.0",
    "description": "Symfony bundle for importing and exporting entities",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hugo SEIGLE",
            "email": "hugo.seigle03@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "phpoffice/phpspreadsheet": "^1.23",
        "doctrine/orm": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SymfonyImportExportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyImportExportBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.* || 7.*"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit --configuration phpunit.xml.dist"
    }
}
